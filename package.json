{
  "name": "paddy-mern",
  "version": "1.0.0",
  "description": "cnc app",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "server": "nodemon src/server.js",
    "test": "NODE_ENV=test jest --watchAll --verbose --coverage",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"cd client && npm run start\""
    
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.4.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.3", 
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
